	<script type="text/ng-template" id="/visita.html">
			<div class="roya-wrap">
						<h2 class="section-header">
		  				Visita Tecnica
		  			</h2>
					<div class="container">
						<div class="row">
							<div class="col-md-6 col-md-offset-3">
								<div class="visita-wrapper">
									<h2>Resultados</h2>
									<div class="visita-results">
										<div class="list-group">
											  <button type="button" class="list-group-item"  data-toggle="modal" data-target="#myModal">Unit Name</button>
											  <button type="button" class="list-group-item"  data-toggle="modal" data-target="#myModal">Another Unit Name</button>
											</div>
											
											<div class="modal fade" id="myModal" tabindex="-1" role="dialog" aria-labelledby="myModalLabel">
											  <div class="modal-dialog" role="document">
											    <div class="modal-content">
											      <div class="modal-header">
											        <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
											        <h4 class="modal-title" id="myModalLabel">Unit Name</h4>
											      </div>
											      <div class="modal-body">
												      <ul class="list-group">
													      <li class="list-group-item">
														   Nombre:  <span class="badge">Pena blanca</span>
														  </li>
														  
														  <li class="list-group-item">
														   Municipio: <span class="badge">Cuilapa</span>
														  </li>
														  
														  <li class="list-group-item">
														   Departamento: <span class="badge">Santa Rosa</span>
														  </li>
														</ul>
												    
														<h3>Recomendacion</h3>
													
													<textarea class="form-control" rows="3"></textarea>
											      </div>
											      <div class="modal-footer">
											        <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
											        <button type="button" class="btn btn-default">Sent recommendation</button>
											      </div>
											    </div>
											  </div>
										</div>
									</div>
									<div class="visita-controls">
										<form class="form" id="search-units">
										<div class="input-group">
									      <input type="text" class="form-control" placeholder="{{ searchmode }}">
									      <span class="input-group-btn">
									        <button class="btn btn-default" type="button">Buscar</button>
									      </span>
									    </div>
									    
									    
										</form>
										<div class="row visita-controls-opt">
									    		<input type="radio" name="inlineRadioOptions" id="inlineRadio1" value="Id" checked ng-model="searchmode">
										    	<label class="col-xs-6" for="inlineRadio1">
												   Por ID
												</label>
												
												<input type="radio" name="inlineRadioOptions" id="inlineRadio2" ng-model="searchmode" value="Usuario">
										    	<label class="col-xs-6" for="inlineRadio2">
												   Por Usuario
												</label>
									    </div>
									</div>
								</div>
							</div>
						</div>						
					</div>
				</div>
		  </script>